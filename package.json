{
    "name": "export-eslint-config",
    "version": "1.1.5",
    "description": "ðŸ”¨ Export configuration from any eslint-config-* to .eslintrc file",
    "license": "MIT",
    "author": {
        "name": "Piotr Kowalski",
        "email": "piecioshka@gmail.com",
        "url": "https://piecioshka.pl"
    },
    "scripts": {
        "test": "jasmine src/**.spec.js",
        "test:verbose": "jasmine src/**.spec.js --reporter=jasmine-console-reporter",
        "coverage": "nyc npm run test && nyc report --reporter=html"
    },
    "devDependencies": {
        "jasmine": "^5.1.0",
        "jasmine-console-reporter": "^3.1.0",
        "nyc": "^17.1.0"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/piecioshka/export-eslint-config.git"
    },
    "files": [
        "bin",
        "src",
        "package.json",
        "README.md",
        "!**/*.spec.js"
    ],
    "keywords": [
        "cli",
        "lint",
        "eslint",
        "export",
        "config",
        "eslintrc"
    ],
    "bin": {
        "export-eslint-config": "bin/cli.js"
    }
}
