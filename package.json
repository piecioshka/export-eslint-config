{
    "name": "export-eslint-config",
    "version": "1.0.0",
    "description": "Export configuration from any eslint-config-* to .eslintrc file",
    "license": "MIT",
    "author": {
        "name": "Piotr Kowalski",
        "email": "piecioshka@gmail.com",
        "url": "http://piecioshka.pl"
    },
    "preferGlobal": true,
    "bin": {
        "export-eslint-config": "index.js"
    },
    "keywords": [
        "lint",
        "eslint",
        "export",
        "config",
        "eslintrc"
    ],
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/piecioshka/export-eslint-config.git"
    },
    "scripts": {
        "test": "jasmine JASMINE_CONFIG_PATH=test/jasmine.json",
        "coverage": "istanbul cover --include-all-sources jasmine JASMINE_CONFIG_PATH=test/jasmine.json test/spec/*"
    },
    "dependencies": {
        "colors": "^1.1.2"
    }
}
